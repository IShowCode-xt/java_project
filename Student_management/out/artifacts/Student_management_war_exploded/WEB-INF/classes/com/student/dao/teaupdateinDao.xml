<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.student.dao.teaupdateinDao">

    <update id="updateIn" parameterType="com.student.po.Teacher">
        update tea set username=#{username},classId=#{classId}
        where id=#{id}
    </update>
</mapper>