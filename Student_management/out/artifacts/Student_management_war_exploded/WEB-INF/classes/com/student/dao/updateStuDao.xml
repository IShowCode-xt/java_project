<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.student.dao.updateStuDao">

    <update id="updateStu" parameterType="com.student.po.Student">
        update stu set username=#{username},idcard=#{idcard},birthdate=#{birthdate},classId=#{classId},password=#{password}
        where id=#{id}
    </update>
</mapper>