<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.student.dao.addStuDao">


    <insert id="addStu" parameterType="string" >
        insert into stu(username,idcard,birthdate,classId,password)values (#{username},#{idcard},#{birthdate},#{classId},#{password})
    </insert>
</mapper>