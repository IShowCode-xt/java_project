<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.student.dao.updateScoreDao">

    <update id="updateScore" parameterType="com.student.po.Score">
     update score set username=#{username},javaw=#{javaw},javaee=#{javaee},vue=#{vue},russion=#{russion}
     where stuCard=#{stuCard}

    </update>
</mapper>